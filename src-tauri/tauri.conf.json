{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Windeck",
  "mainBinaryName": "Windeck",
  "version": "1.0.0",
  "identifier": "com.kr8hu.windeck",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": true,
        "resizable": false,
        "title": "Windeck",
        "width": 1280,
        "height": 800,
        "center": true,
        "useHttpsScheme": true
      }
    ],
    "withGlobalTauri": false,
    "security": {
      "csp": null
    }
  }
}